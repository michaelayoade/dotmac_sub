{
  "max_concurrent_agents": 2,
  "max_retries": 1,
  "agent_timeout_minutes": 30,
  "worktree_base": "agent",
  "auto_review": true,
  "auto_cleanup_days": 7,
  "quality_gates": [
    "shellcheck",
    "pytest",
    "eslint",
    "go-vet"
  ],
  "quality_gate_retries": 1,
  "max_review_cycles": 1,
  "queue_enabled": true,
  "max_queue_size": 50,
  "heartbeat_timeout_minutes": 15,
  "stale_state_minutes": 20,
  "model_learning_enabled": true,
  "log_retention_days": 14,
  "queue_dispatch_batch": 1,
  "engine": "aider",
  "codex_model": "o3",
  "claude_model": "sonnet",
  "frontend_audit_enabled": false,
  "single_executor_mode": false,
  "single_executor_engine": "codex",
  "coordinator_engine": "claude",
  "coordinator_model": "sonnet",
  "pm_engine": "claude",
  "pm_model": "sonnet",
  "fleet_enabled": true,
  "fleet_db_path": "/home/dotmac/projects/.seabone-fleet/fleet.db",
  "fleet_lease_ttl_seconds": 1800,
  "coordinator_mode": "deterministic",
  "taskspec_scoring": {
    "weights": {
      "impact": 35,
      "risk_reduction": 25,
      "urgency": 25,
      "effort_inverse": 15
    }
  },
  "queue_policy": {
    "enabled": true,
    "track_capacity": {
      "reactive": 100,
      "feature": 50,
      "quality": 10,
      "coverage": 8,
      "refactor": 7,
      "reliability": 15,
      "integration": 10,
      "journey": 10,
      "maintenance": 5
    }
  },
  "shared_context_max_entries": 300,
  "codex_only_execution": false,
  "review_model": "opus",
  "review_models": {
    "multi_review_enabled": true,
    "models": [
      "opus"
    ],
    "consensus_threshold": 1
  },
  "aider_model": "openrouter/minimax/minimax-m2.5",
  "review_engine": "claude",
  "escalation_engine": "claude",
  "escalation_model": "opus",
  "engine_routing": {
    "deps": "aider",
    "docs": "aider",
    "formatting": "aider",
    "lint-fix": "aider",
    "readme": "aider",
    "changelog": "aider",
    "test-scaffold": "aider",
    "security": "aider",
    "quality": "aider",
    "api": "aider",
    "coverage": "aider",
    "refactor": "aider",
    "feature": "aider"
  },
  "plan_before_execute": true,
  "plan_engine": "claude",
  "plan_model": "sonnet"
}
